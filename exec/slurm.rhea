#!/bin/bash
#SBATCH -J clustra
#SBATCH -A gen011
#SBATCH -D /home/ost
#SBATCH -t 01:30:00
#SBATCH -N 1
#SBATCH -n 1
#SBATCH -c 32
#SBATCH --mem=4G
#SBATCH -e clustra.e
#SBATCH -o clustra.o

hostname
cd ~/SciApplications/VA/clustra
pwd

## modules can vary on platforms
module load gcc/6.2.0
module load r/4.0.0-py3
module list

## prevent warning when fork is used with MPI
export OMPI_MCA_mpi_warn_on_fork=0

Rscript R/clustra.R
