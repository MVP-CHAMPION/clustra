citHeader("To cite clustra in publications use:")

citEntry(entry = "Misc",
  title        = "Clustering trajectories: {clustra}",
  author       = personList(as.person("George Ostrouchov"),
                   as.person("Hanna Gerlovin"),
                   as.person("David Gagnon"),
                   as.person("Drew Schmidt")),
  url          = "available soon",

  textVersion  =
  paste("George Ostrouchov, Hanna Gerlovin, David Gagnon, Drew Schmidt (2021).",
        "Clustering trajectories: {clustra}",
        "available soon")
)